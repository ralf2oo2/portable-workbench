      "ralf2oo2.portableworkbench.events.init.GuiHandlerListener"
    ],
    "stationapi:event_bus_client": [
    ],
    "stationapi:event_bus_server": [
    ],
    "alwaysmoreitems:plugin": [
      "ralf2oo2.portableworkbench.compat.ami.AMIPlugin"
    ]
  },
  "mixins": [
    "portableworkbench.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}
