{
  "schemaVersion": 1,
  "id": "portableworkbench",
  "version": "${version}",

  "name": "Portable Workbench",
  "description": "This mod adds portable workbenches to the game",
  "authors": [
    "ralf2oo2"
  ],
  "contact": {
    "sources": "https://github.com/ralf2oo2/portable-workbench"
  },

  "license": "MIT",
  "icon": "assets/portableworkbench/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "ralf2oo2.portableworkbench.PortableWorkbench",
      "ralf2oo2.portableworkbench.events.init.ItemListener",
      "ralf2oo2.portableworkbench.events.init.GuiHandlerListener"
    ],
    "stationapi:event_bus_client": [
    ],
    "stationapi:event_bus_server": [
    ],
    "alwaysmoreitems:plugin": [
      "ralf2oo2.portableworkbench.compat.ami.AMIPlugin"
    ]
  },
  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}
